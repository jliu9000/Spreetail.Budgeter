

<div id="purchase-sum-modal" tabindex="-1" class="modal fade" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Goto Purchase Summary</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="item-errors" class="form-group"></div>

                <div class="form-group">
                    <label for="ps-start-date">Start Date</label>
                    <input id="ps-start-date" class="form-control" type="date" />
                </div>

                <div class="form-group">
                    <label for="ps-end-date">End Date</label>
                    <input id="ps-end-date" class="form-control" type="date" />
                </div>

                <div class="modal-footer">
                    <button type="button" id="purchase-sum-go" class="btn btn-primary">Go</button>
                    <button type="button" id="purchase-sum-cancel" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                </div>
            </div>



        </div>
    </div>
</div>
